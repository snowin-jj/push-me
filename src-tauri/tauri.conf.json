{
    "build": {
        "beforeDevCommand": "pnpm dev",
        "beforeBuildCommand": "pnpm build",
        "devPath": "http://localhost:1420",
        "distDir": "../dist"
    },
    "package": {
        "productName": "Push Me"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            },
            "dialog": {
                "all": true
            },
            "window": {
                "setAlwaysOnTop": true,
                "setFocus": true,
                "close": true,
                "hide": true,
                "show": true,
                "maximize": true,
                "minimize": true,
                "unmaximize": true,
                "unminimize": true,
                "startDragging": true
            },
            "notification": {
                "all": true
            },
            "os": {
                "all": false
            }
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "devsnow.in",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": true,
            "endpoints": [
                "https://github.com/snowin-jj/push-me/releases/latest/download/latest.json"
            ],
            "dialog": true,
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEE3MTQ2MTRERUIzNTIzMjEKUldRaEl6WHJUV0VVcHpoUXhNT2c4UitqdGUwbEhqVXNwR2VvTlBYYlhQdTdNSHFoWTBMVVNMMWkK"
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "Push Me",
                "decorations": false,
                "width": 280,
                "height": 200,
                "center": true,
                "transparent": true
            }
        ],
        "macOSPrivateApi": true
    }
}
